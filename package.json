{
  "author": {
    "name": "Michael Akiliev"
  },
  "bugs": {
    "url": "https://github.com/houd1ni/fela-vue/issues"
  },
  "bundleDependencies": [],
  "deprecated": false,
  "description": "Fela mixin for Vue designed for flexibility yet team-oriented.",
  "homepage": "https://houd1ni.github.io/fela-vue/",
  "keywords": [
    "vue",
    "vuejs",
    "fela",
    "css-in-js",
    "css",
    "styles",
    "styling",
    "easy",
    "structure",
    "strongly-typed",
    "typescript",
    "d.ts"
  ],
  "license": "MIT",
  "main": "dist/bundle.js",
  "module": "dist/bundle.esm.js",
  "name": "fela-vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/houd1ni/fela-vue.git"
  },
  "scripts": {
    "lint": "tslint src/*.ts",
    "test": "npm run gentypes && npm run prod:cjs && ava",
    "test:report": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:lazy": "ava",
    "gentypes": "dts-bundle-generator -o dist/bundle.d.ts src/main.ts",
    "dev": "cross-env NODE_ENV=development BUILD=es rollup -c",
    "prod:cjs": "cross-env NODE_ENV=production BUILD=cjs rollup -c",
    "prod:es": "cross-env NODE_ENV=production BUILD=es rollup -c",
    "prod": "npm run gentypes && npm run prod:es && npm run prod:cjs"
  },
  "version": "2.6.0beta.10",
  "ava": {
    "files": [
      "./test/specs/*.ts"
    ],
    "serial": false,
    "failFast": true,
    "timeout": "2m",
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "fela": "*",
    "fela-dom": "*",
    "fela-plugin-embedded": "*",
    "fela-plugin-extend": "*",
    "fela-plugin-fallback-value": "*",
    "fela-plugin-prefixer": "*",
    "fela-plugin-unit": "*"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "ava": "^1.4.1",
    "codecov": "^3.3.0",
    "cross-env": "^5.2.0",
    "dts-bundle-generator": "^3.0.0",
    "nyc": "^14.1.0",
    "rollup": "^1.11.3",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-resolve-aliases": "^0.2.0",
    "rollup-plugin-terser": "4.0.4",
    "rollup-plugin-typescript2": "^0.21.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "types": "./dist/bundle.d.ts"
}
